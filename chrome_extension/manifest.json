{
  "manifest_version": 3,
  "name": "FreshLense Monitor",
  "version": "1.0",
  "description": "Monitors the current page for content changes.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:8000/*"
  ],
 "action": {
    "default_popup": "popup.html"
  }
}